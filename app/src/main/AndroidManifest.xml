<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.taylar.minecraftmoddinghelper" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/DarkNoActionBar" >
        <activity
            android:name=".TestActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".WebActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_web" >
        </activity>
        <activity
            android:name=".Home_Activity"
            android:label="@string/title_activity_home_" >
        </activity>
        <activity
            android:name=".basic_menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ReferenceActivity"
            android:label="@string/title_activity_reference" >
        </activity>
    </application>

</manifest>
